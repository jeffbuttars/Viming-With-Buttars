# main()
snippet :
	${1}:${2};
snippet #
	${1}#${2} {
		${3}
	}

snippet .
	${1}.${2} {
		${3}
	}
snippet border 
	border: ${1:1px} ${2:solid} ${3:#000};
snippet col 
	color: ${1:#fff};
snippet bcol 
	background-color: ${1:#fff};
snippet bg
	background: ${1};
snippet marg 
	margin: ${1:1em} ${2:1em} ${3:1em} ${4:1em};
snippet pad 
	padding: ${1:1em} ${2:1em} ${3:1em} ${4:1em};
snippet pos
	position: ${1};
