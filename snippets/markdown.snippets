# Default style for Cradlepoint documents
snippet cpstyle
	<style>
		body { background-color:#FFF; max-width:960px; }
		h1   { color:#AB3533; }
		h2   { color:#333; }
		h3, h4 { margin-top:2em; color:#511; }
		a    { text-decoration:none; color:#7F2623; }
		li a, li a:visited { color:#AB3533; }
		img .diagram {
			width:600px;
			border:solid #000 1px;
			padding:1em;
			margin:0 0 20em 0;
		}
		body hr {
			color:#7F2824;
			background-color:#7F2824;
			height:1px;
			margin: 3em 0 3em 0;
		}
		pre code {
			-webkit-border-radius: 5px;
			-moz-border-radius: 5px;
			border-radius: 5px;
			display:block;
			padding:5px;
			background-color:#f0f0f0;
			border: solid 1px #aaa;
			font-family: {monospace};
		}
		pre#copyright {
			-webkit-border-radius: 5px;
			-moz-border-radius: 5px;
			border-radius: 5px;
			padding:.2em;
			background-color:#eee;
			font-size:.8em;
			font-family: {sans-serif};
		}
	</style>
snippet pre


	<pre id="${1}">
		${2}
	</pre>

snippet code


	<pre><code class="${1:python}">
		${2}
	</code></pre>

snippet ic
	\`\`${1:code}\`\`${2}

snippet img 


	<img src="${1}" />

snippet table


	<table>
		<thead>
			<tr>
				<th>${1}</th><th>${2}</th><th>${3}</th>
			</tr>
		</thead>
	<!--
		<tfoot>
			<tr>
				<td></td><td></td><td></td>
			</tr>
		</tfoot>
	-->
		<tbody>
			<tr>
				<td>${4}</td><td>${5}</td><td>${6}</td>
			</tr>
		</tbody>
	</table>

	${7}
