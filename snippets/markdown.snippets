# [link][]
snippet []
	[${1:link_id}][]${2}
# [link][id]
snippet [[
	[${1:link}][${2:id}]${3}
# [link](url)
snippet [(
	[${1:link}](${2:url})${3}
# [link](email)
snippet [@
	[${1:link}(mailto:${2:email})${3}
# [link](url "title")
snippet [("
	[${1:link}](${2:url} "${3:title}")${4}
# [id]: url "title"
snippet [:
	[${1:id}]: ${2:url} "${3:title}"${4}
# [id_hash]: urlfromclipboard "title"
snippet [:*
	[${1:id}]: ${2:`@*`} "${3:title}"${4}

# [alttext](imagepath "title")
snippet ![
	![${1:alttext}](${2:/images/image.jpg} "${3:title}")${4}
# [alttext](imagepathfromclipboard "title")
snippet ![*
	![${1:alt}](${2:`@*`} "${3:title}")${4}

snippet ![:
	![${1:id}]: ${2:url} "${3:title}"${4}
snippet ![:*
	![${1:id}]: ${2:`@*`} "${3:title}"${4}

snippet ===
	`repeat('=', strlen(getline(line(".") - 1)))`
	
	${1}
snippet ---
	`repeat('-', strlen(getline(line(".") - 1)))`
	
	${1}

